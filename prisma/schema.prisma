generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma/client"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}



model User {
  id         Int              @id @default(autoincrement())
  email      String
  password   String
  mentee     Mentee?
  mentor     Mentor?
  language   UserLanguage[]
  majors     UserMajor[]
  skills     UserSkills[]
  university UserUniversity[]
}

model Mentor {
  id                  Int        @id @default(autoincrement())
  bio                 String?
  job_title_primary   String
  job_title_secondary String?
  preferred_services  Services[]
  birthyear           Int
  degree_type         String
  highest_education   Int
  name                String
  years_experience    Int
  school_year         Int?
  userId              Int        @unique
  user                User       @relation(fields: [userId], references: [id])
}

model Mentee {
  id                  Int        @id @default(autoincrement())
  bio                 String?
  job_title_primary   String?
  job_title_secondary String?
  preferred_services  Services[]
  school_year         Int?
  birthyear           Int
  degree_type         String
  highest_education   Int
  name                String
  years_experience    Int
  userId              Int        @unique
  user                User       @relation(fields: [userId], references: [id])
}

model University {
  id              Int              @id @default(autoincrement())
  name            String
  city            String[]
  province        String
  country         String
  language        String
  category        String
  undergrad_count Int
  postgrad_count  Int
  total_count     Int
  year_founded    Int
  size_score      Int
  users           UserUniversity[]
}

model Majors {
  id      Int         @id @default(autoincrement())
  major   String      @unique
  faculty String
  users   UserMajor[]
}

model Skills {
  id         Int          @id @default(autoincrement())
  skill      String
  skill_type Skill_Type
  family     Family
  role       String[]
  purpose    String[]
  users      UserSkills[]
}

model Language {
  id         Int            @id @default(autoincrement())
  language   String         @unique
  country    String
  continent  String
  population Int
  users      UserLanguage[]
}

model UserUniversity {
  userId       Int
  universityId Int
  university   University @relation(fields: [universityId], references: [id])
  user         User       @relation(fields: [userId], references: [id])

  @@id([userId, universityId])
}

model UserLanguage {
  userId     Int
  languageId Int
  language   Language @relation(fields: [languageId], references: [id])
  user       User     @relation(fields: [userId], references: [id])

  @@id([userId, languageId])
}

model UserMajor {
  userId  Int
  majorId Int
  major   Majors @relation(fields: [majorId], references: [id])
  user    User   @relation(fields: [userId], references: [id])

  @@id([userId, majorId])
}

model UserSkills {
  userId  Int
  skillId Int
  skill   Skills @relation(fields: [skillId], references: [id])
  user    User   @relation(fields: [userId], references: [id])

  @@id([userId, skillId])
}

